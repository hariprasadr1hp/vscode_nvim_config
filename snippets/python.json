	"jtimeit":{
		"prefix": "jtimeit",
		"body": [
			"def timeit(method):",
			"\tdef timed(*args, **kwargs):",
			"\t\ttstart = time.time()",
			"\t\tresult = method(*args, **kwargs)",
			"\t\ttend = time.time()        ",
			"\t\tprint(\"Time taken for running '{}': {} second(s)\".format(method.__name__, (tend-tstart)))",
			"\t\treturn result",
			"\treturn timed",
			"$0"
		],
		"description": "decorator for timeit"
	},
	
